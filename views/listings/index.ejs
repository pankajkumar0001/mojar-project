<% layout("/layout/bollerpiate") %>
<body>
    <h3>ALL Listing</h3>
   
   
    <div class="row row-cols-lg-3 row-cols-md-3 row-cols-sm-3">
        <% for(let listing of alllistings ){ %> 
            <a href="/listings/<%= listing._id %>" class="listing-click">
            <div class="card">
                <img class="card-img-top" src="<%=listing.image%>"" alt="listing_image"style="height:17rem";>
                <div class="card-img-overlay"></div>
                <div class="card-body">
                  <p class="card-text">
                   <b> <%= listing.title %><br /></b>
                    &#8377;<%= listing.price%>/night
                  </p>
                </div>
              </div>
            </a>
              <% }%>

    </div>

   
</body>
